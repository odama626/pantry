{
	"name": "pantry",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typegen": "source ./.env && pocketbase-typegen --url https://pb.pantry.lilbyte.dev --email $POCKETBASE_SERVICE_USER --password $POCKETBASE_SERVICE_PASSWORD --out ./src/lib/server/db.types.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.0.1",
		"@sveltejs/adapter-vercel": "2.4.2",
		"@sveltejs/kit": "^1.15.8",
		"@types/color-hash": "^1.0.2",
		"@types/lodash-es": "^4.17.7",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"pocketbase-typegen": "^1.1.7",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-typeahead": "^4.4.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.1",
		"vitest": "^0.30.1",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@picocss/pico": "^1.5.10",
		"ag-grid-community": "^29.3.3",
		"color-hash": "^2.0.2",
		"date-fns": "^2.29.3",
		"jsonwebtoken": "^9.0.0",
		"lodash-es": "^4.17.21",
		"nanoid": "^4.0.2",
		"pocketbase": "^0.14.0",
		"qr-scanner": "^1.4.2",
		"qrcode": "^1.5.3",
		"sass": "^1.62.0",
		"the-noun-project": "^2.1.0"
	},
	"pnpm": {
		"patchedDependencies": {
			"svelte-typeahead@4.4.1": "patches/svelte-typeahead@4.4.1.patch",
			"color-hash@2.0.2": "patches/color-hash@2.0.2.patch"
		}
	}
}
